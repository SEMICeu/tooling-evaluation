@prefix ex: <http://example.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dct: <http://purl.org/dc/terms/> .

ex:myOntology a owl:Ontology ;
    dct:title "Core Person Vocabulary"@en ;
    dct:description "This a description"@en .

ex:Person a rdfs:Class, sh:NodeShape ;
    rdfs:label "Person" ;
    rdfs:comment "This is a person class" ;
    sh:property <https://semiceu.github.io/prop/1> ;
    sh:targetClass ex:Person .

<https://semiceu.github.io/prop/1> a sh:PropertyShape ;
    rdfs:seeAlso "https://semiceu.github.io/Core-Person-Vocabulary/releases/2.1.1/#Person.familyname" ;
    sh:datatype rdf:langString ;
    sh:description "The hereditary surname of a family."@en ;
    sh:name "family name"@en ;
    sh:path foaf:familyName .