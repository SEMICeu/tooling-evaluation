@prefix ex: <http://example.org/vocab#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Class definition
ex:Person rdf:type rdfs:Class .

ex:Address rdf:type rdfs:Class .


# Property definition
ex:deathDate rdf:type rdf:Property ;
        rdfs:domain ex:Person ;
        rdfs:range xsd:date .

ex:birthDate rdf:type rdf:Property ;
        rdfs:domain ex:Person ;
        rdfs:range xsd:date .

ex:fullName rdf:type rdf:Property ;
        rdfs:domain ex:Person ;
        rdfs:range xsd:string .

ex:familyName rdf:type rdf:Property ;
        rdfs:domain ex:Person ;
        rdfs:range xsd:string .

ex:givenName rdf:type rdf:Property ;
        rdfs:domain ex:Person ;
        rdfs:range xsd:string .

ex:birthName rdf:type rdf:Property ;
        rdfs:domain ex:Person ;
        rdfs:range xsd:string .

ex:postName rdf:type rdf:Property ;
        rdfs:domain ex:Address ;
        rdfs:range xsd:string .

ex:postCode rdf:type rdf:Property ;
        rdfs:domain ex:Address ;
        rdfs:range xsd:integer .


# Relationship Definition
     
ex:domicile rdf:type rdf:Property ;
    rdfs:domain ex:Address ;
    rdfs:range ex:Person .

# Enumeration definition
